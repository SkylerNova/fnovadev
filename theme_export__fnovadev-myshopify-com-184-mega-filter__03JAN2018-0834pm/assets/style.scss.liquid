.address_actions .action_edit{  border-right: solid 1px {{settings.content_text_color}};  padding-right: 15px; margin-right: 11px; }
.action_top{  margin: 0 0 20px 0; }
.action_edit, .action_delete, .action_add {  display: inline-block; line-height: 17px; }
