#blog.template-list .article-date span.day {font-size: 22px; margin-bottom: 2px;}
.isNotMobile #blog.template-list .article:hover .article-date {background-color: {{settings.content_main_color}}; color: #fff;}

/**** Article ****/

#article {margin: 30px auto 50px auto;}
#article .wrapper {float: left; width: 73%;}
#article h1 {font-size: 18px; text-align: center; border-bottom: 1px solid #eee; padding-bottom: 20px; margin-bottom: 30px;}
