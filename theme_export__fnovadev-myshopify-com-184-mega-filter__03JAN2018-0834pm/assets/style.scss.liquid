.product-item {position: relative; text-align: center; overflow: hidden; margin: 25px 0;}
.product-item .image {overflow: hidden; text-align: center; position: relative;}
.product-item .image img {width: 100%; display: block}
.product-item .image.show-second-image .second-img {position:absolute; top: 0; left:0; opacity: 0; filter: alpha(opacity=0); background-color: #fff; height: 100%; width: 100%;}
.isNotMobile .product-item .image.show-second-image:hover .img {opacity: 0.5; filter: alpha(opacity=50); }
.isNotMobile .product-item .image.show-second-image:hover .img.second-img {opacity: 1; filter: alpha(opacity=100); }
.product-item .image .sale {opacity:1; filter: alpha(opacity = 100); right: 10px; top: 10px; position: absolute; height:42px; width:42px; border-radius:50%; background-color: #000; color:#fff;  }
.product-item .image .sale div { position: relative; top: 11px; font-size: 10px; letter-spacing: 1px; text-align:center;}
.product-item .image .sold-out {opacity:1; filter: alpha(opacity = 100); right: 9px; top: 10px; position: absolute; height:42px; width:42px; border-radius:50%; background-color: {{settings.content_red_details_color}}; color:#000;}
.product-item .image .sold-out div { position: relative; top: 12px; font-size: 10px; letter-spacing: 1px; line-height: 10px; text-align:center;}
.product-item .image .new {opacity:1; filter: alpha(opacity = 100); right: 9px; top: 10px; position: absolute; height:42px; width:42px; border-radius:50%; background-color: #dcdcdc; color:#000;  }
.product-item .image .new div { position: relative; top: 11px; font-size: 10px; letter-spacing: 1px; text-align:center;}
.product-item .title {text-transform: {{ settings.heading_font_transform }}; text-align: center; font-size: 12px; margin: 5px 0 0 0; font-weight: bold; line-height: 16px;}
.product-item .available-more-colors {font-size: 9px; text-transform: uppercase; color: #999;}
.isNotMobile .product-item:hover .title {color: {{ settings.content_main_color}}}
.product-item .quick-view-btn,
.product-item .notify-me{position: absolute; cursor: pointer; bottom: 50px; opacity: 0; text-align: center; filter: alpha(opacity=0); width: 100%; height: 40px; background: rgba(0,0,0,0.7);max-width: 150px;left: 50%;transform: translateX(-50%);}
.product-item .notify-me{display:block!important;background: rgba(65,186,66,.8);font-weight:400; letter-spacing: 0.15em;color: #fff!important;font-size: 10px;text-transform: uppercase;line-height:40px;}
.product-item .quick-view-btn div/*,
.product-item .notify-me span*/{position: relative; font-weight: 200; letter-spacing: 0.15em; color: #fff; top: 26%; font-size: 10px; text-transform: uppercase;}
/*.product-item .notify-me span{font-weight:400;}*/
.product-item .quick-view-btn div i {margin-left: 0; font-size: 14px; top: 3px; position: relative;}
.isNotMobile .product-item:hover .quick-view-btn,
.isNotMobile .product-item:hover .notify-me{opacity: 1; filter: alpha(opacity=100); bottom: 30px;}
.product-item .price {margin-top: 6px; font-size: 14px; color: {{ settings.content_text_color}}; }
.product-item .price .retail {display:inline-block; margin-right: 15px; color: #000; text-decoration:line-through;}  
.product-item .price .retail span {color: #000;}  
.product-item .price .deal {display:inline-block; color: {{ settings.content_text_color}}}
.product-item .price .deal span.red-price {color: #000;     font-weight: 600;}
.product-item .off-percent {float: right; font-size: 15px; text-align: right; color: {{ settings.content_text_color }}; }

.thumbs-swatches-wrap {padding: 20px; padding-bottom: 0; text-align: center;}
.thumbs-swatches-wrap div {cursor: pointer; display: inline-block; box-shadow: 0 0 0 1px #ddd; margin-right: 4.5%; margin-left: -4px; width: 12%; padding-top: 12%; overflow: hidden; background-size: cover; background-position: center; background-repeat: no-repeat;}
.thumbs-swatches-wrap div.feat {margin-right: -4px;}
.thumbs-swatches-wrap div.first {margin-left: 4.5%;}
.thumbs-swatches-wrap div.active {box-shadow: 0 0 0 1px #999;}
.thumbs-swatches-wrap span.more {display: inline-table; position: relative; top: -12px; width: auto; padding: 0; color: {{settings.content_main_color}}; font-size: 0.7vw; margin-left: -4px; margin-top: 2px; text-decoration: underline;}

.product-item .layer.low-stock{position: absolute; bottom: 0; text-align: center; width: 100%; height: 20px; background: rgba(0,0,0,0.7);}
.product-item .layer.low-stock div{position: relative; font-weight: 200; letter-spacing: 0.15em; color: #fff; font-size: 10px; text-transform: uppercase;}
.product-item .color-swatches{font-size:0;letter-spacing:0;line-height:normal;}
.product-item .color-swatches a{display:inline-block;line-height:normal;}
.product-item .color-swatches .color-swatch {margin:5px; width: 20px; height: 20px; border: 1px solid #dedede;border-radius:30px;float:none;display:inline-block;}
.product-item .color-swatches .more-color {color: #8e8e8e; font-size: 12px; margin-bottom: 6px;}

