#blog.template-list .pic-wrapper img {width: 100%}
#blog.template-list .text-wrapper {width: 100%; margin: 25px 0 0 0; font-size: 14px; letter-spacing: 0.03em; line-height: 21px;}
#blog.template-list .text-wrapper .content {margin-bottom: 15px;}
#blog.template-list .article-date {background-color: #fff; border: 4px solid {{settings.content_main_color}}; position: absolute; left: -20px;  top: 85px; text-transform: uppercase;  color: {{settings.content_main_color}};  width: 60px;  height: 50px;  padding-top:10px;  font-size: 18px;  line-height: 18px;  font-weight: 400;  text-align: center;}
