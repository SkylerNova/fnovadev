.collection-top {margin-top: 30px; background-color: #F8F8F8; padding: 18px 0;}
.collection-top .container {padding: 0; max-width: 350px; margin: auto;}
.collection-top select {width: 100%; border: 0; background-color: #fff;}
.collection-top .per-row {float: left; border: 1px solid black; box-sizing: border-box;margin-right: 5%; width: 25%;}
.collection-top .sort-by {border: 1px solid black; width: 100%;}
.collection-top .sort-by label {display: none;}
.collection-top .refine {width: 35%; float: left; border: 1px solid black; box-sizing: border-box; background-color: #000; color: #fff; text-align: center; position: relative; height: 33px; line-height: 30px; font-weight: bold;}
.collection-top .refine .done {display: none;}


.collection-top .refine-menu {display: none; position: absolute; z-index: 1; border: 1px solid black; top: 31px; background-color: #fff; color: #000; left: -1px; width: 250px; padding: 15px; padding-top: 7px;}
.collection-top .refine-menu .submenu {display: block}
.collection-top .refine-menu .title {border-bottom: 1px solid #ccc; padding: 5px; font-size: 13px; margin-top: 8px;}
.collection-top .refine-menu .title i {margin-left: auto; margin-top: 5px; font-size: 18px; color: {{ settings.content_main_color }}}
.collection-top .refine-menu .group {padding: 0px 5px;}
.collection-top .refine-menu .group > div {font-size: 13px; padding: 5px 0;}
.collection-top .refine-menu .group.first {padding-top: 10px;}
.collection-top .refine-menu .filter {background-color: #656565; margin-right: 10px; margin-bottom: 10px; color: #fff; padding: 0px 10px; font-size: 10px;}
.collection-top .refine-menu .filter:first-of-type {margin-left: 0;}
.collection-top .refine-menu .active .filter {opacity: 0.5}

