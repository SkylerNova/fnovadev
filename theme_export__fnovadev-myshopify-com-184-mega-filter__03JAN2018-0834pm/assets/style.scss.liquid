/*
/******************************** RESET *****************************
*/
html, body, div, span, object, iframe, p, blockquote, pre,
abbr, address, cite, code, del, dfn, img, ins, kbd, q, samp,
small, sub, sup, var, dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figcaption, figure,
footer, header, hgroup, menu, nav, section, summary,
time, mark, audio, video { margin: 0;  padding: 0;  border: 0;  font-size: 100%;  font: inherit;  vertical-align: baseline; }
article, aside, details, figcaption, figure,
footer, header, hgroup, menu, nav, section  { display: block; }
hr { display: block; height: 1px; border: 0; border-top: 1px solid #ccc; margin: 1em 0; padding: 0; }
input, select { vertical-align: middle; }
pre, code, kbd, samp { font-family: monospace, sans-serif; }
html { overflow-y: scroll; }
ol { list-style-type: decimal; list-style-position: inside;}
p { margin: 0; }
nav ul, nav li { margin: 0; list-style:none; list-style-image: none; }
small { font-size: 85%; }
strong, th { font-weight: bold; }
td { vertical-align: top; }
sub, sup { font-size: 75%; line-height: 0; position: relative; }
sup { top: -0.5em; }
sub { bottom: -0.25em; }
pre { white-space: pre; white-space: pre-wrap; word-wrap: break-word; padding: 15px; }
textarea { overflow: auto; }
.ie6 legend, .ie7 legend { margin-left: -7px; }
input[type="radio"] { vertical-align: text-bottom; }
input[type="checkbox"] { vertical-align: bottom; }
.ie7 input[type="checkbox"] { vertical-align: baseline; }
.ie6 input { vertical-align: text-bottom; }
label, input[type="button"], input[type="submit"], input[type="image"], button { cursor: pointer; }
button, input, select, textarea { margin: 0; }
input.error, textarea.error { border-color:red }
a {color: inherit; text-decoration: none;}
input[type="image"]{ border: none; }
.ie7 img { -ms-interpolation-mode: bicubic; }
table{  width: 100%;  border-collapse: collapse; vertical-align: middle; border-spacing: 0; }
table td, table th {vertical-align: middle; }
img {display: inline-block;}
ul {list-style-position: inside;}


/*
/******************************** BASE CONTENT STYLES *****************************
*/

html,body { background: {{settings.bg_color}}; -webkit-text-size-adjust: 100%; }
body{ margin: 0px; font-family: {{ settings.body_font }}; font-weight: {{ settings.body_font_weight }}; font-style: {{ settings.body_font_style }}; font-size: {{ settings.body_font_size }}; color: {{ settings.content_text_color}}; letter-spacing: {{settings.body_font_letter_spacing}}; line-height: 1.5em; -webkit-font-smoothing: antialiased;}
#page {background-color: {{settings.bg_color}};}
_:-ms-input-placeholder, :root #page {display:block!important;}
.content-wrapper {background: {{settings.bg_color}}; }
.container {max-width: {{settings.container_width}}; margin: auto; padding: 0 20px; }

{% if settings.header_behavior != "fixed" %}
@media all and (min-width: 1079px) {
.content-wrapper {padding-top: 0 !important;}
}
{% endif %}

h1, h2, h3, h4, h5, h6, .title { color: {{ settings.content_heading_color }}; font-family: {{ settings.heading_font }}; font-weight: {{ settings.heading_font_weight }}; font-style: {{ settings.heading_font_style }}; text-transform: {{ settings.heading_font_transform }}; margin: 0; letter-spacing: {{settings.heading_font_letter_spacing}}; line-height: 1em;  }
h1, h2 {font-size: 26px; text-align: center; padding-bottom: 20px; margin-bottom: 30px; font-weight: bold;}
p {font-size: {{ settings.body_font_size }}; margin: 0 0 15px 0; color: {{ settings.content_text_color}}; font-family: {{ settings.body_font }}; font-weight: {{ settings.body_font_weight }}; font-style: {{ settings.body_font_style }}; letter-spacing: {{settings.body_font_letter_spacing}}; line-height: 1.7em; }
li {font-size: {{ settings.body_font_size }}; font-family: {{ settings.body_font }}; font-weight: {{ settings.body_font_weight }}; font-style: {{ settings.body_font_style }}; letter-spacing: {{settings.body_font_letter_spacing}}; line-height: 1.7em; }
.isNotMobile a:hover { color: {{ settings.content_main_color}} }
input, textarea{font-family: {{ settings.body_font }}; font-weight: {{ settings.body_font_weight }}; font-style: {{ settings.body_font_style }}; color: {{ settings.content_text_color}}; }
input[type="text"], input[type='text'],input[type='email'], input[type="tel"], input[type="phone"], input[type='password'], input[type="submit"], input[type="button"] {appearance:none; -moz-appearance:none; /* Firefox */ -webkit-appearance:none; /* Safari and Chrome */ border-radius: 0; background: transparent; border: 0;}
input[type="submit"], input[type="button"], button {cursor: pointer;}
input:-webkit-autofill, textarea:-webkit-autofill, select:-webkit-autofill {background-color: transparent !important; -webkit-box-shadow: 0 0 0px 1000px white inset; box-shadow: 0 0 0px 1000px white inset;}
input[type=number]::-webkit-inner-spin-button, 
input[type=number]::-webkit-outer-spin-button {opacity: 1;}

a {-webkit-transition: all 0.2s linear; -moz-transition: all 0.2s linear; -o-transition: all 0.2s linear; -ms-transition: all 0.2s linear; transition: all 0.2s linear;}
.animation {-webkit-transition: all 0.2s linear; -moz-transition: all 0.2s linear; -o-transition: all 0.2s linear; -ms-transition: all 0.2s linear; transition: all 0.2s linear;}
.animation-slow {-webkit-transition: all 0.4s linear; -moz-transition: all 0.4s linear; -o-transition: all 0.4s linear; -ms-transition: all 0.4s linear; transition: all 0.4s linear;}
.animation-slower {-webkit-transition: all 0.8s linear; -moz-transition: all 0.8s linear; -o-transition: all 0.8s linear; -ms-transition: all 0.8s linear; transition: all 0.8s linear;}
.animation-fast {-webkit-transition: all 0.1s linear; -moz-transition: all 0.1s linear; -o-transition: all 0.1s linear; -ms-transition: all 0.1s linear; transition: all 0.1s linear;}
.ease-animation {-webkit-transition: all .2s ease-out; -moz-transition: all .2s ease-out; -o-transition: all .2s ease-out; transition: all .2s ease-out; }
.ease-animation-med {-webkit-transition: all .4s ease-out; -moz-transition: all .4s ease-out; -o-transition: all .4s ease-out; transition: all .4s ease-out; }
.ease-animation-slow {-webkit-transition: all .5s ease-out; -moz-transition: all .5s ease-out; -o-transition: all .5s ease-out; transition: all .5s ease-out; }

:focus {outline:none;}
::selection {background: {{ settings.content_main_color}}; color: #fff;}
::-moz-selection {background: {{ settings.content_main_color}}; color: #fff}
::-webkit-input-placeholder {color: {{settings.content_placeholder_color}};}
:-moz-placeholder {color: {{settings.content_placeholder_color}};}
::-moz-placeholder {color: {{settings.content_placeholder_color}};}
:-ms-input-placeholder {color: {{settings.content_placeholder_color}};}

input:-webkit-autofill, textarea:-webkit-autofill, select:-webkit-autofill {background-color: transparent !important; -webkit-box-shadow: 0 0 0px 1000px white inset; box-shadow: 0 0 0px 1000px white inset;}

.horizontal-line {border-bottom: 1px solid #333; line-height: .1em; margin: 25px 0; text-align: center; width: 100%;}
.horizontal-line span {background-color: #fff; padding: 0 10px;}

/**** SELECT STYLES ****/

select {appearance:none; -moz-appearance:none; /* Firefox */ -webkit-appearance:none; /* Safari and Chrome */ border-radius: 0;}
select {padding: 8px 25px 8px 15px; box-sizing: border-box; border: 1px solid #eee; min-width: 1px; width: 190px; color: {{settings.content_text_color}}; overflow: hidden; background: transparent url({{"select.png" | asset_url}}) no-repeat 96% center; background-size: auto 50%; box-shadow: none; font-family: {{ settings.body_font }}; -webkit-appearance: none; -moz-appearance: none; appearance: none;} 
select::-ms-expand {display: none;}


/**** INPUT STYLES ****/

input[type='text'],input[type='email'], input[type="tel"], input[type="phone"], input[type='password'], textarea {border: 1px solid #eee; box-sizing: border-box; padding: 15px 20px; width: 100%; color: {{settings.content_text_color}}; font-size: {{settings.body_font_size}};  }
textarea{border: 1px solid #eee; height: 130px; padding: 15px 20px; background: transparent }


/**** BUTTON STYLES ****/

.btn, a.btn, input.btn {
  display: inline-block;
  background: {{settings.button_color}};
  border: none;
  -webkit-border-radius: {{settings.button_radius}};
  -moz-border-radius: {{settings.button_radius}};
  border-radius: {{settings.button_radius}};
  color: #fff;
  padding: {{settings.button_padding}};
  font-size: {{settings.button_font_size}};
  text-decoration: none;
  text-transform: {{settings.button_transform}};
  font-weight: {{settings.button_weight}};
  font-family: {% if settings.menu_font == "body" %}{{ settings.body_font }}{% else %}{{ settings.heading_font }}{% endif %};
  font-style: normal;
  text-align: center;
  letter-spacing: {{settings.button_spacing}};
  cursor: pointer;
  -webkit-transition: all 0.2s linear; 
  -moz-transition: all 0.2s linear;
  -o-transition: all 0.2s linear;
  -ms-transition: all 0.2s linear;
  transition: all 0.2s linear;
}
	
.isNotMobile .btn:hover,.isNotMobile a.btn:hover, .isNotMobile input.btn:hover {opacity: 0.7; filter: alpha(opacity= 70); }

.btn-notice {background: {{settings.notice_button_color}};}
.btn.secondary {background: {{settings.secondary_button_color}};}


/**** CUSTOM CHECKBOX AND RADIO STYLES ****/

.checkbox-custom, .radio-custom {opacity: 0; position: absolute;}
.checkbox-custom, .checkbox-custom-label, .radio-custom, .radio-custom-label {display: inline-block; vertical-align: middle; cursor: pointer;}
.checkbox-custom-label, .radio-custom-label { position: relative;}
.checkbox-custom + .checkbox-custom-label:before {float: left; content: ''; background: #fff url({{"checked.png" | asset_url}}) no-repeat center; background-size: 0%; border: 2px solid #e5e5e5; display: inline-block; vertical-align: middle; width: 18px; height: 18px; margin-right: 15px; text-align: center; -webkit-transition: all 0.2s linear; -moz-transition: all 0.2s linear; -o-transition: all 0.2s linear; -ms-transition: all 0.2s linear; transition: all 0.2s linear; }
.radio-custom + .radio-custom-label:before {float: left; content: ''; border: 2px solid #e5e5e5; border-radius: 50%; display: inline-block; vertical-align: middle; width: 18px; height: 18px; margin-right: 15px; text-align: center; -webkit-transition: all 0.2s linear; -moz-transition: all 0.2s linear; -o-transition: all 0.2s linear; -ms-transition: all 0.2s linear; transition: all 0.2s linear;}
.checkbox-custom:checked + .checkbox-custom-label:before {background-color: #e5e5e5; background-size: 70%;}
.radio-custom + .radio-custom-label:before { border-radius: 50%;}
.radio-custom:checked + .radio-custom-label:before { background: {{settings.content_main_color}}; box-shadow: inset 0px 0px 0px 4px #fff;}

/*
/******************************** SCROLL TO TOP *****************************
*/

.scrollup {display: none; position: fixed; z-index: 90; bottom: 20px; right:10px; border: 1px solid #000;}
.scrollup a {background: {{ settings.content_main_color}}; display: block; width: 34px; height: 34px; color:#fff; font-size:26px; text-align:center; text-decoration:none; border-radius: 0;}
.scrollup i {padding: 3px 0 0 0;}
.isNotMobile .scrollup a:hover {color: #fff;}

/*
/******************************** OVERLAYS *****************************
*/

#page::after {position: fixed; top: 0; right: 0; width: 0; height: 0; background: rgba(0,0,0,0.7); content: ''; opacity: 0; -webkit-transition: opacity 0.5s, width 0.1s 0.5s, height 0.1s 0.5s; transition: opacity 0.5s, width 0.1s 0.5s, height 0.1s 0.5s; z-index: 4000;}
#page.mode-overlay::after {width: 100%; height: 100%; opacity: 1; -webkit-transition: opacity 0.5s; transition: opacity 0.5s;}
#add-cart-overlay {width: 100%; height: 100%; position: fixed; top: 0; left: 0; background-color: #000; z-index: 999999; opacity: 0.4; text-align: center; padding-top: 250px; display: none;}
#loaderImage {top: 50%; left: 50%; position: fixed; margin-top: -40px; margin-left: -40px;}

/*
/******************************** HEADER *****************************
*/

.header {color: {{settings.header_text_color}}; position: relative; flex-shrink: 0;}

#header.over-carousel {color: #fff; position: absolute; z-index: 100; background: transparent; border: none;}
#header.over-carousel .menu-container .has-dropdown ul {background: transparent;}

#header {width: 100%; background: {{settings.header_color}}; border-bottom: 1px solid {{settings.header_border_color}}; text-align: center; position: relative;}
#header .header-wrapper {margin: 0 auto; padding: 24px 0;}

{% if settings.header_behavior == "fixed" %}
#header {position: fixed; z-index: 100;}
{% endif %}

#header .logo {margin-top: {{settings.logo_mar_top}}; text-align: left;}
#header .logo img {max-width: {{settings.logo_max_width}}; max-height: {{settings.logo_max_height}}; width: 100%; margin: auto;}
#header .table-cell.logo {width: 20%; min-width: 360px;}
#header .table-cell.search {width: 60%;}
#header .table-cell.utils-wrapper {width: 30%; min-width: 380px;}

#header .cart-wrap {margin-left: 15px;}
#header .cart-wrap .count, #header .wishlistLink .count {position: relative; padding: 5px 8px; background: {{ settings.content_main_color }}; color: #fff; font-size: 11px; margin-left: 5px;}
#header .cart-wrap .count:before, #header .wishlistLink .count:before {content: ''; display: block; position: absolute; right: 100%; top: 50%; margin-top: -5px; border-right: 5px solid {{ settings.content_main_color }}; border-top: 5px solid transparent; border-bottom: 5px solid transparent;}

#header .top-header {position: relative; padding-top: 15px; font-size: 11px !important; font-weight: {{settings.menu_font_weight}};}
#header .top-header .top-utils {}
#header .top-header .top-utils ul {list-style-type: none;}
#header .top-header .top-utils li {float: right; margin: 0; margin-left: 10px; text-align: center; letter-spacing: 0; font-size: 14px !important; font-weight: normal; min-width: 130px;}
#header .top-header .top-utils li.user {margin-left: 0; min-width: 100px;}
#header .top-header .top-utils li.cart-wrap {text-align: center;}
#header .top-header .top-utils i {margin-right:4px}

#header .top-utils .floating-cart {border: 1px solid #000; width: 240px; right: 0; position: absolute; background: #fff; z-index: 99; margin-top: 8px; padding: 10px; color: #000; text-align: center; display: none;}
#header .top-utils .floating-cart .grid {margin-left: -10px;}
#header .top-utils .floating-cart .grid__item {padding-left: 10px;}
#header .top-utils .floating-cart .head {margin-bottom: 10px;}
#header .top-utils .floating-cart img {width: 100%;}
#header .top-utils .floating-cart .title {text-align: left; text-transform: initial;}
#header .top-utils .floating-cart .price {text-align: left; color: {{ settings.content_main_color }}; margin-top: 5px;}
#header .top-utils .floating-cart .price .money {display: block;}
#header .top-utils .floating-cart .buttons {margin-top: 10px;}
#header .top-utils .floating-cart .buttons .btn {display: block; padding: 6px 0px;}
#header .top-utils .floating-cart .continue-shopping {margin-top: 6px;}

#header .top-header .social-top {float: right; font-size: 12px !important; }
#header .top-header .social-top #social-links {list-style:none; margin: 0px; }
#header .top-header .social-top #social-links li {display:inline-block; margin: 0; font-size: 12px !important; }
#header .top-header .social-top #social-links.awesome i {}
#header .top-header .social-top #social-links.awesome li {margin-right: 15px;}
#header .top-header .social-top #social-links.awesome li:hover i {color: {{settings.content_main_color}};}

#header .searchbar {padding-top: 10px;}
#header .searchbar input {padding: 8px 12px; line-height: 20px;}
#header .searchbar .btn {padding: 9px 20px; padding-bottom: 10px; display: block}

.shipping-banner{background:#F4F4F4;text-transform:uppercase;padding:7px 0;color: {{settings.header_text_color}}!important; }
.shipping-banner .grid__item{border-left:1px solid #000;padding-top:3px;padding-bottom:3px;}
.shipping-banner .grid__item:first-child{border:none;}

/*
/******************************** HEADER DROP *****************************
*/

.header-drop {position: fixed; width: 100%; height: 56px; box-shadow: 0 0 10px rgba(0,0,0,0.1); background-color: {{ settings.header_color }}; top: -67px; z-index: 999;}
.header-drop.scroll {top: 0;}
.header-drop .header-wrapper {margin: 0 auto; position: relative;}
.header-drop .logo {float: left; margin-top: {{settings.drop_logo_mar_top}}; text-align: left}
.header-drop .logo img {max-width: 130px; max-height: 40px; }
.header-drop .top-utils {float: right; font-size: {{settings.menu_font_size}}; font-weight: {{settings.menu_font_weight}}; margin-top: 0; position: relative; top: 17px; right: 0; width: 90px;}
.header-drop .top-utils ul {list-style-type: none;}
.header-drop .top-utils li {float: left; margin-left: 0px;}
.header-drop .top-utils li span {display: none;}
.header-drop .cart-wrap {margin-left: 15px;}
.header-drop .main-menu {float: left; margin-left: 40px; background-color: #fff; color: #000;}
.header-drop .main-menu ul li div {padding-bottom: 12px;}


/*
/******************************** FULL SCREEN SEARCH AND MAILING LIST *****************************
*/

.search-mailing .form .input-icon {position: absolute; right: 0; font-size: 30px; top: 22px; width: auto; height: auto; padding: 0;}
.search-mailing {position: fixed; z-index: 4000; width: 100%; height: 100%; display: none; top: 0; left: 0; background: rgba({{settings.search_mailing_bg}});}
.search-mailing i.lnr-cross {position: fixed; display: none; z-index: 99997; top: 30px; right: 30px; color: {{settings.search_mailing_text}}; font-size: 35px; cursor: pointer}
.search-mailing .form {display: none; position: fixed; z-index: 99997; width: 50%; margin-left: -25%; left: 50%; height: 80px; top: 30%; margin-top: -40px; }
.search-mailing .form input { color: {{settings.search_mailing_text}}; font-size: 50px; padding: 0 0 10px 7px; width: 100%; border: 0; background: transparent; -webkit-appearance: none; box-sizing: border-box; outline: 0; font-weight: 100;}
.search-mailing .form ::-webkit-input-placeholder {color: {{settings.search_mailing_text}};}
.search-mailing .form :-moz-placeholder {color: {{settings.search_mailing_text}}; opacity: 1;}
.search-mailing .form ::-moz-placeholder {color: {{settings.search_mailing_text}}; opacity: 1;}
.search-mailing .form :-ms-input-placeholder {color: {{settings.search_mailing_text}};}
.search-mailing .success { color: {{settings.search_mailing_text}}; font-size: 50px; padding: 0 0 10px 7px; width: 100%; line-height: 1em; border: 0; background: transparent; box-sizing: border-box; outline: 0; font-weight: 100;}


/******************************** UTILITY BAR *****************************
*/

#utility-bar {background: {% if settings.utility_bar_bacckground_image %}url({{ 'utility_bar.jpg' | asset_url }}){% endif %} {{settings.utility_bar_color}}; position: relative; color: {{settings.utility_bar_text_color}}; overflow: hidden; padding: 6px 0; text-align: center; font-size: 12px; }
#utility-bar a:hover {color: {{settings.utility_bar_text_color}};}
#utility-bar .container {position: relative; padding: 0 40px; max-width: 2000px; height: auto !important;}
#utility-bar i {position: absolute; color: {{settings.utility_bar_close_color}}; right: 20px; top: 50%; font-size: 16px; margin-top: -9px; }
#utility-bar.image {padding: 0; }
#utility-bar img {margin: auto; max-width: 100%; margin-bottom: -5px;}
#utility-bar img.small--hide {margin-bottom: -5px;}


/*
/******************************** MENU *****************************
*/

.main-menu {background-color: #000; color: #fff;}
.main-menu ul {display: inline-block; list-style-type: none;}
.main-menu ul li {display: inline-block; position: relative; letter-spacing: {{settings.menu_letter_spacing}}; text-transform: {{ settings.menu_text_transform }}; font-family: {% if settings.menu_font == "body" %}{{ settings.body_font }}{% else %}{{ settings.heading_font }}{% endif %}; font-size: 14px; font-weight: {{settings.menu_font_weight}}; margin-right: 20px; }
.main-menu ul li > div {padding: 14px 0;}
.main-menu ul li.active  {}
.main-menu li.last {margin-right: 0;}

/* dropdown menus */
.main-menu .nav-arrow{ position: absolute; top: 0; right: 10px; width: 0; height: 0; border-left: 4px solid transparent; border-right: 4px solid transparent; border-top: 4px solid {{ settings.content_text_color}}; display: block; }
.main-menu .has-dropdown > div {clear: none; position: absolute; z-index: 30; background: {{ settings.dropdown_bg_color }}; top: 100%; left: -20px; padding: 5px 0; color: #000;}
.main-menu .has-dropdown .first-level {/*width: 440px;*/width:200px; padding: 25px; padding-left: 0;}
.main-menu .has-dropdown .first-level .items {padding-top: 0; padding-bottom: 0; /*width: 40%;*/}
.main-menu .has-dropdown .first-level .images {width: 58%; border-left: 1px solid #ccc; padding-left: 20px; box-sizing: border-box;}
.main-menu .has-dropdown .first-level .images img {width: 100%;}
.main-menu .has-dropdown .arrow {display: none; content: ''; border-bottom: 10px solid #fff; border-left: 10px solid transparent; border-right: 10px solid transparent; position: absolute; bottom: 0px; left: 0px;}
.main-menu .has-dropdown.active .arrow {display: block;}
.main-menu .has-dropdown li{text-align: left; display: block; text-transform: {{ settings.menu_text_transform }}; font-size: 14px; font-weight: 500; margin: 0; }
.main-menu .has-dropdown li div {padding: 0 !important;}
.main-menu .has-dropdown li.last {border-bottom: 0px;}
.main-menu .has-dropdown li a{position: relative; margin: 0; box-sizing: border-box; min-width: 170px; padding: 6px 20px; display: block; border: none; padding-right: 0px;}
.main-menu .has-dropdown .arrow {font-size: 14px; margin-left: 3px;}
.main-menu .has-dropdown li .arrow-right {position: absolute; right: 20px; top: 50%; margin-top: -8px; font-size: 14px; margin-left: 3px;}
.main-menu .has-dropdown i {position: relative; top: -1px;}

@media only screen and (min-width: 1600px){
  .main-menu li:nth-child(n+12) .has-dropdown > div {left: auto;right: 0px;}
  .main-menu li.last .has-dropdown > div {left: -20px!important;right: auto!important;}
}
@media only screen and (min-width: 1750px){
  .main-menu li.last .has-dropdown > div {left: auto!important;right: 0px!important;}
}
/* level 3 */
.main-menu .has-dropdown ul .has-dropdown ul {top: -5px !important; left: 170px;}

/*
/****************************** FOOTER *********************************
*/

#footer {width: 100%; color: {{settings.content_footer_text_color}}; margin: 0; padding: 40px 0; text-align: center; background-color: {{ settings.footer_bg_color }}; border-top: 0px solid {{ settings.footer_border_color }}; flex-shrink: 0;}
#footer .copy {color: {{settings.content_footer_text_color}}; font-size: 11px; opacity: 0.7; filter: alpha(opacit=0); font-weight: 500; text-align: center; margin: 0 auto;}

#footer #social-links {margin-bottom: 25px;max-width: 140px;font-size: 0;}
@media only screen and (min-width: 1500px){
  #footer #social-links {max-width: none;}
}
#footer #social-links i {width: 35px; height: 35px; border-radius: 100%; color: {{ settings.footer_bg_color }}; background: {{settings.content_footer_text_color}}; font-size: 18px; box-sizing: border-box; padding-top: 9px;}
.isNotMobile #footer #social-links i:hover {background: {{settings.content_main_color}};}
#footer #social-links li {margin-right: 5px; text-align: center;margin-right: 10px;margin-top:5px;margin-bottom: 5px;}
#footer #social-links li:last-child {margin-right: 0;}

