/**** Blog GRID layout ****/

#blog.template-grid {margin: 30px auto 50px auto;}
#blog.template-grid h2 {text-align: left; font-size: 16px; padding: 0; border: none; margin: 0; margin-bottom: 20px; font-weight: 700; }
#blog.template-grid .article {position: relative; margin-bottom: 30px;}
#blog.template-grid .pic-wrapper {width: 100%; position: relative;}
#blog.template-list .pic-wrapper .image {height: 330px; overflow: hidden; background-size: cover; background-position: center; background-repeat: no-repeat;}
#blog.template-grid .pic-wrapper img {width: 100%; display: block;}
#blog.template-grid .pic-wrapper .layer {opacity: 0; position: absolute; top: 0; left: 0; width: 100%; height: 100%; background: rgba(0, 0, 0, 0.4); letter-spacing: 1px; color: #fff;}
#blog.template-grid .pic-wrapper .layer .title {width: 100%; position: absolute; top: 40%; margin-top: -26px; padding: 20px 0; background: {{settings.content_main_color}}; color: inherit;}
#blog.template-grid .pic-wrapper .layer .title h3 {font-size: 12px; font-weight: 500; color: inherit; text-align: center; width: 85%; margin: 0 auto;}
.isNotMobile #blog.template-grid .article:hover .pic-wrapper .layer {opacity: 1;}
.isNotMobile #blog.template-grid .article:hover h2 {color: {{settings.content_main_color}};}
.isNotMobile #blog.template-grid .article:hover .pic-wrapper .layer .title {top: 50%;}
#blog.template-grid .text-wrapper {width: 100%; margin: 25px 0 0 0; font-size: 14px; letter-spacing: 0.03em; line-height: 21px;}
#blog.template-grid .text-wrapper .content {margin-bottom: 10px;}
